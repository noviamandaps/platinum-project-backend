  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.7.1",
    "supertest": "^6.3.3"
  }

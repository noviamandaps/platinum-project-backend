    "on-headers": "^1.0.2",
    "path": "^0.12.7",
    "sequelize": "^6.25.7",
    "sequelize-cli": "^6.5.2",
    "socket.io": "^4.5.4",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0",
    "tsscmp": "^1.0.6"
